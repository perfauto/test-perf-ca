              <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="${step_name} ${DS_Name}:  - Populate connection string template with real values" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">url = vars.get(&apos;urlTemplate&apos;)

url = url.replaceAll(&quot;(?i)&lt;hostname&gt;&quot;,vars.get(&quot;ServerName&quot;))
url = url.replaceAll(&quot;(?i)&lt;host&gt;&quot;,vars.get(&quot;ServerName&quot;))
url = url.replaceAll(&quot;(?i)&lt;port&gt;&quot;,vars.get(&quot;Port&quot;))
url = url.replaceAll(&quot;(?i)&lt;databasename&gt;&quot;,vars.get(&quot;DBName&quot;))
url = url.replaceAll(&quot;(?i)&lt;instancename&gt;&quot;,vars.get(&quot;InstanceName&quot;))
url = url.replaceAll(&quot;(?i)&lt;logintimeout&gt;&quot;,vars.get(&quot;LoginTimeout&quot;))
url = url.replaceAll(&quot;(?i)&lt;oracleserviceid&gt;&quot;,vars.get(&quot;OracleServiceID&quot;))
url = url.replaceAll(&quot;(?i)&lt;hostDescription&gt;&quot;,vars.get(&quot;ServerName&quot;) + &quot;:&quot; + vars.get(&quot;Port&quot;))
url = url.replaceAll(&quot;INFORMIXSERVER=&lt;server-name&gt;&quot;,&quot;INFORMIXSERVER=&quot; + vars.get(&quot;INFORMIXSERVER&quot;))

url = url.replaceAll(&quot;\\[&quot;,&quot;&quot;).replaceAll(&quot;\\]&quot;,&quot;&quot;).replaceAll(&quot;(?i)&lt;databasename&gt;&quot;,vars.get(&quot;DBName&quot;)) //see defect 199993

vars.put(&quot;connectionURL&quot;,url)

specialParams = &quot;&quot;

if(vars.get(&quot;title&quot;) == &quot;DB2 iSeries&quot;){
	specialParams = &quot;^User ID:^?Password:&quot;
}

vars.put(&quot;specialParams&quot;,specialParams)</stringProp>
              </JSR223Sampler>
